<?xml version='1.0' encoding='utf-8'?>
<widget id="pdavila.todos" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Device">
        <param name="browser-package" value="Device" />
    </feature>
    <feature name="Camera">
        <param name="browser-package" value="Camera" />
    </feature>
    <name>Todos</name>
    <description>
        A ToDos app with VueJS.
    </description>
    <author email="pdavila@iesebre.com" href="http://github.com/pdavila13">
        Paolo DÃ¡vila Develop
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-navigation href="http://todosbackend.pdavila.2dam.acacha.org:8080/*" />
</widget>
